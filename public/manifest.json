{
  "name": "Dallenge",
  "short_name": "Dallenge",
  "description": "Dallenge - Your All-in-One Educational and Wellness Platform",
  "start_url": "/",
  "id": "/?utm_source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay"
  ],
  "background_color": "#000000",
  "theme_color": "#000000",
  "orientation": "any",
  "lang": "en-US",
  "dir": "auto",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "categories": [
    "education",
    "health",
    "social"
  ],
  "screenshots": [
    {
      "src": "/screenshots/screenshot-1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Social Feed"
    },
    {
      "src": "/screenshots/screenshot-2.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Engage in Challenges"
    }
  ],
  "shortcuts": [
    {
      "name": "Open Feed",
      "short_name": "Feed",
      "description": "View the community social feed",
      "url": "/feed",
      "icons": [{ "src": "/icons/shortcut-feed.png", "sizes": "96x96" }]
    },
    {
      "name": "View Profile",
      "short_name": "Profile",
      "description": "Go to your user profile",
      "url": "/profile",
      "icons": [{ "src": "/icons/shortcut-profile.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/share-receiver",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+dallenge",
      "url": "/?deeplink=%s"
    }
  ]
}
