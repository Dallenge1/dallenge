rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read and write access for all users for the next 30 days.
    // WARNING: This is insecure for production.
    // You should secure your database before launching.
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2025, 10, 19);
    }
  }
}
